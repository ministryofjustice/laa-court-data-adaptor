{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/external/apiCourtApplicationType.json",
  "description": "The type of application made to court for judicial decision. Taken from reference data",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/core/courts/external/apiCourtsDefinitions.json#/definitions/uuid"
    },
    "code": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "legislation": {
      "type": "string"
    },
    "categoryCode": {
      "type": "string"
    },
    "linkType": {
      "type": "string",
      "enum": [
        "STANDALONE",
        "LINKED",
        "SJP",
        "FIRST_HEARING"
      ]
    },
    "jurisdiction": {
      "$ref": "http://justice.gov.uk/core/courts/external/apiCourtsDefinitions.json#/definitions/applicationJurisdictionType"
    },
    "appealFlag": {
      "type": "boolean"
    },
    "summonsTemplateType": {
      "type": "string",
      "enum": [
        "GENERIC_APPLICATION",
        "BREACH",
        "NOT_APPLICABLE",
        "FIRST_HEARING",
        "PARENT_GENERIC_CASE"
      ]
    },
    "validFrom": {
      "type": "string",
      "format": "date"
    },
    "validTo": {
      "type": "string",
      "format": "date"
    },
    "applicantAppellantFlag": {
      "type": "boolean"
    },
    "pleaApplicableFlag": {
      "type": "boolean"
    },
    "offenceActiveOrder": {
      "type": "string",
      "enum": [
        "OFFENCE",
        "COURT_ORDER",
        "NOT_APPLICABLE"
      ]
    },
    "commrOfOathFlag": {
      "type": "boolean"
    },
    "breachType": {
      "type": "string",
      "enum": [
        "GENERIC_BREACH",
        "COMMISSION_OF_NEW_OFFENCE_BREACH",
        "NOT_APPLICABLE"
      ]
    },
    "courtOfAppealFlag": {
      "type": "boolean"
    },
    "courtExtractAvlFlag": {
      "type": "boolean"
    },
    "listingNotifTemplate": {
      "type": "string"
    },
    "boxworkNotifTemplate": {
      "type": "string"
    },
    "typeWelsh": {
      "type": "string"
    },
    "legislationWelsh": {
      "type": "string"
    },
    "prosecutorThirdPartyFlag": {
      "type": "boolean"
    },
    "spiOutApplicableFlag": {
      "type": "boolean"
    },
    "hearingCode": {
      "type": "string"
    },
    "resentencingActivationCode": {
      "type": "string"
    },
    "prefix": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "type",
    "categoryCode",
    "linkType",
    "jurisdiction",
    "summonsTemplateType",
    "breachType",
    "appealFlag",
    "applicantAppellantFlag",
    "pleaApplicableFlag",
    "commrOfOathFlag",
    "courtOfAppealFlag",
    "courtExtractAvlFlag",
    "prosecutorThirdPartyFlag",
    "spiOutApplicableFlag",
    "offenceActiveOrder"
  ],
  "additionalProperties": false
}