{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://justice.gov.uk/core/courts/external/apiMarker.json",
    "description": "Markers categorise the classes of a case or a person on a prosecution case",
    "type": "object",
    "properties": {
        "id": {
            "$ref": "http://justice.gov.uk/core/courts/external/apiCourtsDefinitions.json#/definitions/uuid"
        },
        "markerTypeid": {
            "description": "The marker type from reference data",
            "$ref": "http://justice.gov.uk/core/courts/external/apiCourtsDefinitions.json#/definitions/uuid"
        },
        "markerTypeCode": {
            "description": "The Code for the marker type",
            "type": "string"
        },
        "markerTypeDescription": {
            "description": "The Title for the marker type",
            "type": "string"
        }
    },
    "required": [
        "id",
        "markerTypeid",
        "markerTypeCode",
        "markerTypeDescription"
    ],
    "additionalProperties": false
}