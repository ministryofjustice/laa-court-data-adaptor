  "id": "http://justice.gov.uk/core/courts/external/apiLjaDetails.json",
  "description": "Specific data items required by Generating Nows for the given defendant at a sentencing sentencing court",
  "type": "object",
  "properties": {
    "ljaCode": {
      "type": "string"
    },
    "ljaName": {
      "type": "string"
    },
    "welshLjaName": {
      "type": "string"
    }
  },
  "required": [
    "ljaCode",
    "ljaName"
  ],
  "additionalProperties": false
}