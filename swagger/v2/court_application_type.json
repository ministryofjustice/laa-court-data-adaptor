{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "court_application_type",
  "description": "The type of application made to court for judicial decision. Taken from reference data",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "definitions.json#/definitions/uuid"
    },
    "code": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "legislation": {
      "type": "string"
    },
    "category_code": {
      "type": "string"
    },
    "link_type": {
      "type": "string",
      "enum": [
        "STANDALONE",
        "LINKED",
        "SJP",
        "FIRST_HEARING"
      ]
    },
    "jurisdiction": {
      "description": "Indicates the jurisdiction that is required to consider the application",
      "type": "string",
      "enum": [
        "MAGISTRATES",
        "CROWN",
        "EITHER"
      ]
    },
    "appeal_flag": {
      "type": "boolean"
    },
    "summons_template_type": {
      "type": "string",
      "enum": [
        "GENERIC_APPLICATION",
        "BREACH",
        "NOT_APPLICABLE",
        "FIRST_HEARING",
        "PARENT_GENERIC_CASE"
      ]
    },
    "valid_from": {
      "type": "string",
      "format": "date"
    },
    "valid_to": {
      "type": "string",
      "format": "date"
    },
    "applicant_appellant_flag": {
      "type": "boolean"
    },
    "plea_applicable_flag": {
      "type": "boolean"
    },
    "offence_active_order": {
      "type": "string",
      "enum": [
        "OFFENCE",
        "COURT_ORDER",
        "NOT_APPLICABLE"
      ]
    },
    "commr_of_oath_flag": {
      "type": "boolean"
    },
    "breach_type": {
      "type": "string",
      "enum": [
        "GENERIC_BREACH",
        "COMMISSION_OF_NEW_OFFENCE_BREACH",
        "NOT_APPLICABLE"
      ]
    },
    "court_of_appeal_flag": {
      "type": "boolean"
    },
    "court_extract_avl_flag": {
      "type": "boolean"
    },
    "listing_notif_template": {
      "type": "string"
    },
    "boxwork_notif_template": {
      "type": "string"
    },
    "type_welsh": {
      "type": "string"
    },
    "legislation_welsh": {
      "type": "string"
    },
    "prosecutor_third_party_flag": {
      "type": "boolean"
    },
    "spi_out_applicable_flag": {
      "type": "boolean"
    },
    "hearing_code": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "type",
    "category_code",
    "link_type",
    "jurisdiction",
    "summons_template_type",
    "breach_type",
    "appeal_flag",
    "applicant_appellant_flag",
    "plea_applicable_flag",
    "commr_of_oath_flag",
    "court_of_appeal_flag",
    "court_extract_avl_flag",
    "prosecutor_third_party_flag",
    "spi_out_applicable_flag",
    "offence_active_order"
  ],
  "additionalProperties": false
}
